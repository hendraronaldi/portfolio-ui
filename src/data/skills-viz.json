  "projectTransitions": {
    "type": "horizontal-slide",
    "duration": 500,
    "easing": "cubic-bezier(0.4, 0, 0.2, 1)",
    "direction": {
      "forward": "translateX(-100%)",
      "backward": "translateX(100%)"
    },
    "stages": {
      "prepare": {
        "duration": 0,
        "transform": "translateX(100%)",
        "opacity": 1
      },
      "enter": {
        "duration": 500,
        "transform": "translateX(0%)",
        "opacity": 1
      },
      "exit": {
        "duration": 500,
        "transform": "translateX(-100%)",
        "opacity": 1
      }
    },
    "edgeCases": {
      "firstProject": {
        "preventBackward": false,
        "wrapToLast": true
      },
      "lastProject": {
        "preventForward": false,
        "wrapToFirst": true
      }
    },
    "accessibility": {
      "reduceMotion": {
        "enabled": true,
        "fallback": "fade"
      },
      "announcements": {
